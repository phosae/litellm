{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug LiteLLM Proxy (admin.yaml)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/.venv/bin/litellm",
            "args": [
                "--config",
                "zenx/admin.yaml",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "TZ": "Asia/Shanghai",
                "PYTHONPATH": "${workspaceFolder}",
                "STORE_MODEL_IN_DB": "True",
                "LITELLM_LOG": "DEBUG"
            },
            "justMyCode": false,
            "stopOnEntry": false
        },
        {
            "name": "Debug Prisma Script",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/zenx/debug_prisma.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "DATABASE_URL": "postgresql://postgres:root@host.orb.internal:5432/litellm",
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": false,
            "stopOnEntry": false
        }
    ]
} 